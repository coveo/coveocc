<?xml version="1.0" encoding="ISO-8859-1"?>
<items 	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:noNamespaceSchemaLocation="items.xsd">

	<itemtypes>
		<itemtype code="BaseSite" autocreate="false" generate="false">
			<attributes>
				<attribute type="java.lang.String" qualifier="coveoPlatformUrl" generate="true" autocreate="true">
					<description>
						ex https://platform.cloud.coveo.com
					</description>
					<persistence type="property" />
				</attribute>
				<attribute type="java.lang.String" qualifier="coveoApiKey" generate="true" autocreate="true">
					<description>
						the api key with impersonate privilege
					</description>
					<persistence type="property" />
				</attribute>
			</attributes>
		</itemtype>
	</itemtypes>
</items>
